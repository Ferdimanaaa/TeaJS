{
	"name"        : "TeaJS",
	"version"     : "0.0.9",
	"author"      : "Liang Wang",
	"description" : "Unfancy JavaScript like coffeescript",
	"keywords"    : ["javascript", "language", "teascript", "compiler" ],
	"repository"  : {
        "type"    : "git",
        "url"     : "https://github.com/wl879/TeaJS.git"
    },
	"devDependencies" : {
		"source-map"  : "*",
		"uglify-js"   : "*"
	},
	"main"        : "./lib/tea.js",
	"directories" : {
		"lib"     : "./lib/"
	},
	"bin"         : {
		"tea"     : "./lib/index.js"
	},
	"scripts"         : {
		"build": "node ./lib/index.js -p ./src/ -d define.c -f index.tea -o ./bin/tea.js -j -v --clear"
	},
	"license"         : "MIT"
}