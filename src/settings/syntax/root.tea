
Syntax = require "../../core/syntax";

module.exports = function(src, params):
	@.parser('COMMENT', src, null, false, true);
	if src.current && src.current.is('BLANK', 'END', 'COMM', 'COMMENT'):
		@.next(src, 1);
	node = @.parser('Block', src, ['brace'], false, true) || new Syntax('Root');
	node.type = 'Root';
	return node;